<Application x:Class="TestWpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestWpf"
             xmlns:i18nwpf="clr-namespace:EleCho.Internationalization.Wpf;assembly=EleCho.Internationalization.Wpf"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <i18nwpf:I18nStringResources I18nStrings="{x:Static local:App.GlobalStrings}"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
